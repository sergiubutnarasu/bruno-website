{
  "name": "bruno-website",
  "private": true,
  "version": "0.0.1",
  "description": "Bruno UI Kit website",
  "scripts": {
    "build": "stencil build --prerender",
    "start": "stencil build --dev --watch --serve",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {
    "@stencil/core": "0.10.7",
    "@stencil/router": "^0.2.5",
    "@stencil/sass": "0.0.5",
    "bruno-ui": "^0.2.5",
    "highlight.js": "^9.12.0"
  },
  "devDependencies": {
    "@types/jest": "^21.1.1",
    "jest": "^21.2.1"
  },
  "author": "Sergiu Butnarasu",
  "license": "MIT",
  "homepage": "http://bruno.butnarasu.ro",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
